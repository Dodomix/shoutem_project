{
  "name": "example-create-react-app-express",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && yarn start",
    "player": "cd player && yarn start",
    "server": "nodemon server.js",
    "component-server": "nodemon component-server.js",
    "setup-client": "cd client && npm install",
    "setup-player": "cd player && npm install",
    "setup-server": "npm install",
    "setup": "concurrently \"yarn setup-server\" \"yarn setup-client\" \"yarn setup-player\"",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn component-server\" \"yarn client\" \"PORT=3001 yarn player\" \"PORT=3002 yarn player\""
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "chess.js": "^0.10.2",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "concurrently": "^3.6.0"
  }
}
