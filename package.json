{
  "name": "example-create-react-app-express",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && yarn start",
    "content-editor": "cd content-editor && yarn start",
    "style-editor": "cd style-editor && yarn start",
    "malicious-page": "cd malicious-page && yarn start",
    "server": "nodemon server.js",
    "component-server": "nodemon component-server.js",
    "setup-client": "cd client && npm install",
    "setup-content-editor": "cd content-editor && npm install",
    "setup-style-editor": "cd style-editor && npm install",
    "setup-malicious-page": "cd malicious-page && npm install",
    "setup": "npm install && concurrently \"yarn setup-client\" \"yarn setup-content-editor\" \"yarn setup-style-editor\" \"yarn setup-malicious-page\"",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn component-server\" \"yarn client\" \"yarn content-editor\" \"yarn style-editor\" \"yarn malicious-page\""
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0"
  },
  "devDependencies": {
    "concurrently": "^3.6.0"
  }
}
